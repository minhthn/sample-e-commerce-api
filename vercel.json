{
    "name": "sample-e-commerce-api",
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node --experimental-specifier-resolution=node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "src/app.js"
        }
    ]
}
